<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 
  
   
   <ThingDef ParentName="BuildingBase">
    <defName>ElvenPillar</defName>
    <label>Elven Pillar</label>
    <graphicData>
      <texPath>Things/Building/Pillars/Elvenpillar</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,2)</drawSize>
    </graphicData>
	<stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
	<holdsRoof>true</holdsRoof>
	<costStuffCount>50</costStuffCount>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>0</pathCost>
    <fillPercent>0.5</fillPercent>
    <castEdgeShadows>false</castEdgeShadows>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <WorkToBuild>2500</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    
    <description>Decorative, elven roof support.</description>
    <size>(1,1)</size>
	<blockLight>false</blockLight>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <designationCategory>Fences</designationCategory>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <designationHotKey>Misc5</designationHotKey>
   </ThingDef>


